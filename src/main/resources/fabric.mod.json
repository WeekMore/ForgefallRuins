{
	"schemaVersion": 1,
	"id": "forgefallruins",
	"version": "${mod_version}",
	"name": "ForgeFallRuins",
	"description": "A Minecraft Mod featuring Roguelike Game",
	"authors": [
		"yuxiangll",
		"EArias"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/WeekMore/ForgefallRuins"
	},
	"license": "GNU General Public License v3.0",
	"icon": "assets/forgefallruins/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "weekmore.forgefallruins.ForgeFallRuins",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "weekmore.forgefallruins.ForgeFallRuinsDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"forgefallruins.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": "~${minecraft_version}",
		"fabric-api": "${fabric_version}",
		"fabric-language-kotlin": "${fabric_kotlin_version}"
	},
	"suggests": {
		"another-mod": "*"
	}
}